/*
 *  ======== Platform.tci ========
 *
 */

/*
 * The following comment is used by the Graphical Config Tool:
 * !DESCRIPTION 6XXX!  evmDRA459 (552 MHz) with 128 Mbytes SRAM
 * !NAME! ti.platforms.evmDRA459
 */


/*
 * Setup platform-specific memory map, CLK rate, etc.
 */
var mem_ext = [];

mem_ext[0] = {
    comment: "256Mbytes of DSP off-chip memory",
    name: "EMIFA",
    base: 0x40000000,
    len:  0x10000000,
    space: "code/data"
};

mem_ext[1] = {
    comment: "512Mbytes of DSP off-chip memory",
    name: "EMIFB",
    base: 0x80000000,
    len:  0x20000000,
    space: "code/data"
};

/* Specify the L1 and L2 memory settings */
var device_regs = {
    l1PMode: "32k",
    l1DMode: "32k",
    l2Mode: "0k"
};

var params = {
    clockRate: 552,
    catalogName: "ti.catalog.c6000",
    deviceName: "DRA459",
    regs: device_regs,
    mem: mem_ext
};

/*
 * Customize generic platform with parameters specified above.
 */
utils.loadPlatform("ti.platforms.generic", params);
