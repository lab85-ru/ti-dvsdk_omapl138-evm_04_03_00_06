# ***************************************************************************

setenv LO_PATH #<your_pathname>#
setenv LINUX_PATH ${LO_PATH}/bin

if ($?LD_LIBRARY_PATH == 0) then
    setenv LD_LIBRARY_PATH ${LINUX_PATH}
else
    setenv LD_LIBRARY_PATH ${LINUX_PATH}:${LD_LIBRARY_PATH}
endif

${LINUX_PATH}/load6xexe $*

