/*
 *  ======== package.bld ========
 */

var Build = xdc.useModule('xdc.bld.BuildEnvironment');
var Pkg = xdc.useModule('xdc.bld.PackageContents');

/*
 * This package distributes its sources in the default release.  This is to
 * enable better understanding by the customer/field, as well as enable
 * modification and profiling techniques in the field.
 */
Pkg.attrs.exportSrc = true;

Pkg.otherFiles = ["imgdec1.h", "imgenc1.h", "_imgdec1.h", "_imgenc1.h",
        "package.bld", "IIMGDEC1_dsutils.xs", "IIMGENC1_dsutils.xs"];

var DECSRCS = ["imgdec1.c", "imgdec1_skel.c", "imgdec1_stubs.c"];
var ENCSRCS = ["imgenc1.c", "imgenc1_skel.c", "imgenc1_stubs.c"];

for (var i = 0; i < Build.targets.length; i++) {
    var targ = Build.targets[i];

    Pkg.addLibrary("lib/release/imgdec1", targ).addObjects(DECSRCS);
    Pkg.addLibrary("lib/release/imgenc1", targ).addObjects(ENCSRCS);

    Pkg.addLibrary("lib/debug/imgdec1", targ, {
        profile  : "debug"
    }).addObjects(DECSRCS);
    Pkg.addLibrary("lib/debug/imgenc1", targ, {
        profile  : "debug"
    }).addObjects(ENCSRCS);
}
